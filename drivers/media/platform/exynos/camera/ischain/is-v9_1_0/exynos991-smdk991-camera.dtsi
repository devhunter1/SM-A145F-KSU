	};
};

/* PERIC0 USI15_I2C REAR main */
&hsi2c_31 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpp15 2 0 &gpp15 3 0>;
	status = "okay";
	samsung,fast-plus-mode;
	clock-frequency = <1000000>;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c31_bus_in>;
	pinctrl-1 = <&hsi2c31_bus>;
	pinctrl-2 = <&hsi2c31_bus_in>;

	is-gm1sp@2D {
		compatible = "samsung,exynos-is-cis-gm1sp";
		reg = <0x2D>;
		id = <0>; /* matching sensor id */
		setfile = "setB";
	};

	is-actuator@72 {
		compatible = "samsung,exynos-is-actuator-dw9839";
		reg = <0x0C>;
		id = <0>; /* matching is_sensor id */
		place = <0>;
	};
};

/* PERIC0 USI15 REAR sub */
&hsi2c_30 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpp15 0 0 &gpp15 1 0>;
	status = "okay";
	samsung,fast-plus-mode;
	clock-frequency = <1000000>;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c30_bus_in>;
	pinctrl-1 = <&hsi2c30_bus>;
	pinctrl-2 = <&hsi2c30_bus_in>;

	is-5e9@10 {
		compatible = "samsung,exynos-is-cis-5e9";
		reg = <0x10>;
		id = <2>; /* matching sensor id */
		setfile = "setB";
	};
};

/* PERIC1 USI17_I2C front VT0  */
&hsi2c_35 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpp17 2 0 &gpp17 3 0>;
	status = "okay";
	clock-frequency = <400000>;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c35_bus_in>;
	pinctrl-1 = <&hsi2c35_bus>;
