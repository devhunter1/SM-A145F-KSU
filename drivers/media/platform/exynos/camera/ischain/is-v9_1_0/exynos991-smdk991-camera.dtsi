&is_sensor0 {
	#address-cells = <1>;
	#size-cells = <1>;
	scenario = <SENSOR_SCENARIO_NORMAL>;	/* Normal, Vision, OIS etc */
	id = <0>;
	csi_ch = <0>;
	dma_ch = <0 0 0 0>;
	vc_ch = <0 1 2 3>;
	flite_ch = <FLITE_ID_NOTHING>;
	is_bns = <0>;
	status = "okay";
};

&is_sensor1 {
	#address-cells = <1>;
	#size-cells = <1>;
	reg = <0x0 0x17050000 0x1000>, /* MIPI-CSI2 */
		<0x0 0x170D0000 0x10000>; /* PHY: M0S4S4S4S4S4S2 */
	phys = <&mipi_phy_csis2_m0s4s4s4s4s4s2 0>;
	scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
	id = <1>;
	csi_ch = <2>;
	dma_ch = <1 1 1 1>;
	vc_ch = <0 1 2 3>;
	flite_ch = <FLITE_ID_NOTHING>;
	is_bns = <0>;
	status = "okay";

	interrupts = <0 INTREQ__CSIS2 IRQ_TYPE_LEVEL_HIGH>, /* MIPI-CSI2 */
		<0 INTREQ__CSIS_DMA1 IRQ_TYPE_LEVEL_HIGH>,
		<0 INTREQ__CSIS_DMA1 IRQ_TYPE_LEVEL_HIGH>,
		<0 INTREQ__CSIS_DMA1 IRQ_TYPE_LEVEL_HIGH>,
		<0 INTREQ__CSIS_DMA1 IRQ_TYPE_LEVEL_HIGH>;

	/* without PAF HW */
	sensor1_ch_mode0: sensor1-ch-mode0 {
		reg = <0x17098000 0x100>, /* DMA1 VC0 */
			<0x17098400 0x100>, /* DMA1 Common */
			<0x17098100 0x100>, /* DMA1 VC1 */
			<0x17098400 0x100>, /* DMA1 Common */
			<0x17098200 0x100>, /* DMA1 VC2 */
			<0x17098400 0x100>, /* DMA1 Common */
			<0x17098300 0x100>, /* DMA1 VC3 */
			<0x17098400 0x100>; /* DMA1 Common */
		mux_reg = <0x17020424 0x4>; /* DMA1 input mux */
	};
};

&is_sensor2 {
	#address-cells = <1>;
	#size-cells = <1>;
	reg = <0x0 0x17040000 0x1000>, /* MIPI-CSI1 */
		<0x0 0x170D0000 0x10000>; /* PHY: M0S4S4S4S4S4S2 */
	phys = <&mipi_phy_csis1_m0s4s4s4s4s4s2 0>;
	scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
	id = <2>;
	csi_ch = <1>;
	dma_ch = <2 2 2 2>;
	vc_ch = <0 1 2 3>;
	flite_ch = <FLITE_ID_NOTHING>;
	is_bns = <0>;
	status = "okay";

	interrupts = <0 INTREQ__CSIS1 IRQ_TYPE_LEVEL_HIGH>, /* MIPI-CSI1 */
		<0 INTREQ__CSIS_DMA2 IRQ_TYPE_LEVEL_HIGH>,
		<0 INTREQ__CSIS_DMA2 IRQ_TYPE_LEVEL_HIGH>,
		<0 INTREQ__CSIS_DMA2 IRQ_TYPE_LEVEL_HIGH>,
		<0 INTREQ__CSIS_DMA2 IRQ_TYPE_LEVEL_HIGH>;

	/* without PAF HW */
	sensor2_ch_mode0: sensor2-ch-mode0 {
		reg = <0x17099000 0x100>, /* DMA2 VC0 */
			<0x17099400 0x100>, /* DMA2 Common */
			<0x17099100 0x100>, /* DMA2 VC1 */
			<0x17099400 0x100>, /* DMA2 Common */
			<0x17099200 0x100>, /* DMA2 VC2 */
			<0x17099400 0x100>, /* DMA2 Common */
			<0x17099300 0x100>, /* DMA2 VC3 */
			<0x17099400 0x100>; /* DMA2 Common */
		mux_reg = <0x17020428 0x4>; /* DMA2 input mux */
	};
};

